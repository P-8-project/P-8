install_routes: FORCE
	@test -d $(INSTALL_ROOT)/usr/local/etc/p-8 || mkdir -p $(INSTALL_ROOT)/usr/local/etc/p-8
	test -e $(INSTALL_ROOT)/usr/local/etc/p-8/routes || cp -f examples/config/routes $(INSTALL_ROOT)/usr/local/etc/p-8/routes


install_p-8conf: FORCE
	@test -d $(INSTALL_ROOT)/usr/local/etc/p-8 || mkdir -p $(INSTALL_ROOT)/usr/local/etc/p-8
	test -e $(INSTALL_ROOT)/usr/local/etc/p-8/p-8.conf || cp -f p-8.conf.inst $(INSTALL_ROOT)/usr/local/etc/p-8/p-8.conf


install: install_subtargets install_runnerlibfiles install_runnerconfigfiles install_runnerbinfiles install_p-8configfiles install_routes install_p-8conf  FORCE

uninstall: uninstall_runnerlibfiles uninstall_runnerconfigfiles uninstall_p-8configfiles  uninstall_subtargets FORCE

FORCE:

