{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Viable",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#comments"
    },
    {
      "include": "#symbols"
    },
    {
      "include": "#numbers"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.other.viable",
          "match": "\\b(of|capture|to|of|some|match|over|option|not|either)\\b"
        }
      ]
    },
    "symbols": {
      "patterns": [
        {
          "name": "constant.character.viable",
          "match": "(<space>|<newline>|<tab>|<return>|<feed>|<null>|<digit>|<word>|<vertical>)"
        },
        {
          "name": "constant.character.viable",
          "match": "\\b[A-Za-z]\\b"
        },
        {
          "name": "constant.character.viable",
          "match": "\\b(start|end|char)\\b"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.viable",
          "match": "\\d"
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.viable",
          "match": "//.*"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.viable",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.viable",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.single.viable",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.viable",
              "match": "\\\\."
            }
          ]
        }
      ]
    }
  },
  "scopeName": "source.mdy"
}
